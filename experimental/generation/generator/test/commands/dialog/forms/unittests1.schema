{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "properties": {
        "Quantity": {
            "type": "number",
            "minimum": 1,
            "maximum": 10,
            "$entities": [
                "number"
            ]
        },
        "Length": {
            "type": "integer",
            "minimum": 11,
            "maximum": 20,
            "$entities": [
                "integer"
            ]
        },
        "Complete": {
            "type": "boolean",
            "$entities": [
                "boolean"
            ]
        },
        "NameArray": {
            "type": "array",
            "items": {
                "type": "string"
            },
            "$entities": [
                "personName"
            ]
        },
        "Email": {
            "type": "string",
            "format": "email"
        },
        "URL1": {
            "type": "string",
            "format": "uri"
        },
        "URL2": {
            "type": "string",
            "format": "iri"
        },
        "Name": {
            "type": "string",
            "$entities": [
                "personName"
            ]
        },
        "PhoneNumber": {
            "type": "string",
            "$entities": [
                "phonenumber"
            ]
        },
        "KeyphraseProperty": {
            "type": "string",
            "$entities": [
                "keyPhrase"
            ]
        },
        "Percentage": {
            "type": "string",
            "$entities": [
                "percentage"
            ]
        },
        "Age": {
            "$ref": "template:age.schema#/age"
        },
        "OrdinalV2": {
            "$ref": "template:ordinal.schema#/ordinalV2"
        },
        "GeographyV2": {
            "$ref": "template:geography.schema#/geographyV2"
        },
        "Money": {
            "$ref": "template:money.schema#/money"
        },
        "Temperature": {
            "$ref": "template:temperature.schema#/temperature"
        },
        "Dimension": {
            "$ref": "template:dimension.schema#/dimension"
        },
        "Datetime": {
            "$ref": "template:datetime.schema#/datetime"
        }
    },
    "required": [
        "Quantity",
        "Length",
        "Complete",
        "NameArray",
        "Email",
        "URL1",
        "URL2",
        "Name",
        "PhoneNumber",
        "KeyphraseProperty",
        "Percentage",
        "Age",
        "OrdinalV2",
        "GeographyV2",
        "Money",
        "Temperature",
        "Dimension",
        "Datetime"
    ],
    "$requires": [
        "standard.schema"
    ]
}