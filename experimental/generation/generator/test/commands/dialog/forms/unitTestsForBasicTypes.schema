{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "properties": {
        "Name": {
            "type": "string",
            "$entities": [
                "personName"
            ]
        },
        "Quantity": {
            "type": "number",
            "minimum": 1,
            "maximum": 10,
            "$entities": [
                "number"
            ]
        },
        "Length": {
            "type": "integer",
            "minimum": 11,
            "maximum": 20,
            "$entities": [
                "integer"
            ]
        },
        "ItemArray": {
            "type": "array",
            "items": {
                "type": "string",
                "enum": [
                    "apple",
                    "orange",
                    "pear"
                ]
            }
        },
        "NameArray": {
            "type": "array",
            "items": {
                "type": "string"
            },
            "$entities": [
                "personName"
            ]
        },
        "Complete": {
            "type": "boolean",
            "$entities": [
                "boolean"
            ]
        }
    },
    "required": [
        "Name",
        "Quantity",
        "ItemArray",
        "NameArray",
        "Complete",
        "Length"
    ],
    "$requires": [
        "standard.schema"
    ]
}